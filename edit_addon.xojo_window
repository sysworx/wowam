#tag Events btn_ok
	#tag Event
		Sub Action()
		  Dim url as String = txt_url.text
		  Dim source as string
		  Dim h as new whttps
		  h.HTTPProxyAddress = app.proxy_server
		  h.HTTPProxyPort = app.proxy_port
		  Dim sender as new whttps 
		  Dim senddata As New Dictionary
		  h.SetRequestHeader("User-Agent:", "curl/7.21.0 (i686-pc-linux-gnu) libcurl/7.21.0 OpenSSL/0.9.8o zlib/1.2.3.4 libidn/1.18")
		  
		  Dim AddonName as String
		  Dim AddonVersion as String
		  
		  'string not empty
		  if url.Len > 0 then
		    'url contains curse
		    if url.InStr("https://mods.curse.com/addons/wow") >0 then 
		      PreferencesModule.Log("Edit", "-------- Starting Edit Procedure for " + main.listbox1.Cell(listboxRow,0) + " -----------------")
		      'load the website
		      source=h.Get(url,50)
		      'search addon name
		      AddonName = app.getRegex(source,"<meta property=""og:title"" content=""(.+)""")
		      'search addon version
		      AddonVersion = app.getRegex(source,"<li class=""newest-file"">[^:]+: ([^<]+)")
		      'compare the names
		      if listboxAddon = AddonName then
		        'edit the listboxCell
		        main.listbox1.Cell(listboxRow,3) = txt_url.Text
		        listboxRow = -1
		        listboxAddon = ""
		        'send the data to sys-worx
		        senddata.Value("addon") = AddonName
		        senddata.Value("url") = txt_url.Text
		        Dim result As String
		        sender.SetFormData(senddata)
		        PreferencesModule.Log("Edit", "Send data to the WOWAM-Cloud: " + AddonName + " / " + url)
		        result = sender.Post("",5) // Synchronous
		        PreferencesModule.Log("Edit", "Changed url to: " + txt_url.Text)
		        PreferencesModule.Log("Edit", "-------- End Edit Procedure for " + main.listbox1.Cell(listboxRow,0) + " -------------------")
		        'close form
		        edit_addon.close
		      else
		        MsgBox "The URL seems not to be the same Addon as in the list!"
		      end if
		      
		    else
		      Msgbox "This is not a valid url from curse.com!"
		    end if
		  end if
		  
		End Sub
	#tag EndEvent
#tag EndEvents
